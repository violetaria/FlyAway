<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.flyaway.app.MainActivity" tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fly Away"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="16dp"/>
    <Switch
        android:id="@+id/switch1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="One Way" tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="54dp" tools:text="Round Trip"/>
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/from"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        tools:layout_editor_absoluteX="262dp"
        tools:layout_editor_absoluteY="94dp"/>
    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/to"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="94dp"/>
    <EditText
        android:id="@+id/editText"
        android:layout_width="115dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="@string/mm_dd_yy" android:inputType="date"
        tools:layout_editor_absoluteX="233dp" tools:layout_editor_absoluteY="194dp"/>
    <EditText
        android:id="@+id/editText2"
        android:layout_width="115dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="@string/mm_dd_yy" android:inputType="date"
        tools:layout_editor_absoluteX="33dp" tools:layout_editor_absoluteY="194dp"/>
    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:elevation="0dp"
        android:text="@string/search" tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="284dp"/>
    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView"
        android:layout_width="115dp"
        android:layout_height="46dp"
        tools:layout_editor_absoluteX="233dp" tools:layout_editor_absoluteY="134dp"/>
    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView2"
        android:layout_width="115dp"
        android:layout_height="46dp"
        tools:layout_editor_absoluteX="33dp" tools:layout_editor_absoluteY="134dp"/>

</android.support.constraint.ConstraintLayout>
